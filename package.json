{
  "name": "dt-mergebot",
  "version": "1.0.9",
  "description": "dt-mergebot",
  "main": "index.js",
  "dependencies": {
    "@graphql-codegen/fragment-matcher": "^1.8.1",
    "@octokit/graphql-schema": "^6.45.0",
    "@types/node": "latest",
    "@types/node-fetch": "^1.6.7",
    "@types/request": "^2.0.9",
    "apollo-boost": "^0.4.4",
    "definitelytyped-header-parser": "github:Microsoft/definitelytyped-header-parser#production",
    "fs-extra": "^4.0.3",
    "graceful-fs": "^4.1.11",
    "graphql": "^14.5.8",
    "moment": "^2.20.1",
    "node-fetch": "^1.7.3",
    "request": "^2.83.0",
    "treeage": "latest",
    "tslib": "^1.10.0",
    "typescript": "^3.7.0-dev.20191003",
    "idembot": "^0.0.12",
    "chalk": "^3.0.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "dry": "npm run build && node node_modules/idembot/bin/run.js --file bin/rules.js --query all-open-prs.json --dry",
    "wet": "npm run build && node node_modules/idembot/bin/run.js --file bin/rules.js --query all-open-prs.json",
    "single": "npm run build && node node_modules/idembot/bin/run.js --file bin/rules.js --single",
    "single-info": "npm run build && node bin/show-info.js",
    "single-debug": "npm run build && node --inspect-brk node_modules/idembot/bin/run.js --file bin/rules.js --single",
    "cleanproject": "npm run build && node bin/clean-columns.js",
    "graphql-schema": "npx apollo client:codegen schema --target typescript --globalTypesFile=src/schema/graphql-global-types.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCavanaugh/dt-mergebot.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCavanaugh/dt-mergebot/issues"
  },
  "homepage": "https://github.com/RyanCavanaugh/dt-mergebot#readme",
  "devDependencies": {
    "apollo": "^2.18.3"
  }
}
